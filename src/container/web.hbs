<div>{{#with web}}

  <h1>Web</h1>
  <hr>
  <h2>Languages: </h2>

  <section>
    <div>{{#with languages.[0]}}
      {{> web_start_descriptions}}
      <strong>Elements:</strong>
      <div>{{#each elements}}
        {{> web_definition}}
        <ul>
          <h4>Attributes:</h4>
          <div>{{#each attributes}}
            <li>
              {{> web_definition}}
            </li>
            {{/each}}
          </div>
        </ul>
        {{/each}}
      </div>
      {{/with}}
    </div>
  </section>

  <section>
    <div>{{#with languages.[1]}}
      {{> web_start_descriptions}}
      <div>{{#each properties}}
        {{> web_definition}}
        <strong>Values:</strong>
        <ul>
          <div>{{#each values}}
            <li>
              <p><em>{{value}}</em> - {{description}}</p>
            </li>
            {{/each}}
          </div>
        </ul>
        {{/each}}
      </div>
      {{/with}}
    </div>
  </section>

  <section>
    <div>{{#with languages.[2]}}
      {{> web_start_descriptions}}
      <div>{{#each functions}}
        {{> web_definition}}
        <ul>
          <strong>Parameters:</strong>
          <div>{{#each parameters}}
            <li>
              <p><b>Name: </b>{{name}}</p>
            </li>
            <li>
              <p><b>Type: </b>{{type}}</p>
            </li>
            <li>
              <p><b>Description: </b>{{description}}</p>
            </li>
            {{/each}}
          </div>
        </ul>
        {{/each}}
      </div>
      {{/with}}
    </div>
  </section>

  {{/with}}
</div>